pipeline{
       agent {
       docker 
       
      
       stages {
       
            stage('Compile'){
                  steps{ 
                    echo "Compiled successfully"
                               }        
                  }
            stage('to deploy container'){
                       
               steps{ 
                    sshagent(['docker-key']) {
                            sh "docker ps"
                           }
                    }
                    }
          }
          }
